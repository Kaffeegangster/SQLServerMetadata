  <DataSets>
    <DataSet Name="DataSource1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RootObjectID">
            <Value>=Parameters!RootObjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RunKey">
            <Value>=Parameters!RunKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH f(DataFlowID, ParentID, ID, Name, Depth, Walk, RunKey) AS 
(SELECT     dbo.SourceTables.DataFlowID, dbo.SourceTables.ObjectKey, 
			dbo.SourceTables.SrcComponentKey as ID, convert(varchar(max),dbo.SourceTables.SourceObjectsName), 
			0 AS Depth, 
			convert(varchar(max),dbo.SourceTables.SourceObjectsName) as Walk
			, SourceTables.RunKey
			FROM  dbo.SourceTables 
			INNER JOIN dbo.Objects 
				ON dbo.SourceTables.ObjectKey = dbo.Objects.ObjectKey
				AND dbo.SourceTables.RunKey = dbo.Objects.RunKey
			WHERE dbo.SourceTables.SrcComponentKey = @RootObjectID
			AND dbo.SourceTables.RunKey = @RunKey
UNION ALL
SELECT		 f.DataflowID, f.ParentID, 
			dbo.LineageMap.TgtObjectKey AS ID, convert(varchar(max), Objects_1.ObjectName),
			f.Depth + 1 AS Depth, 
			walk+' &lt;--&gt; '+convert(varchar(max), Objects_1.ObjectName)
			, f.RunKey
			FROM    f  
			INNER JOIN dbo.LineageMap 
				ON f.ID = dbo.LineageMap.SrcObjectKey
				AND f.RunKey = dbo.LineageMap.RunKey
			INNER JOIN dbo.Objects AS Objects_1 
				ON dbo.LineageMap.TgtObjectKey = Objects_1.ObjectKey
				AND dbo.LineageMap.RunKey = Objects_1.RunKey),
Dest1 as (select distinct t1.tgtobjectkey as ID from dbo.LineageMap t1 left join dbo.LineageMap t2 on t1.tgtobjectkey = t2.srcobjectkey and t1.RunKey = t2.RunKey
where t2.tgtobjectkey is null)
SELECT     ParentID, ID, Name, Depth, Walk
FROM         f where ID in (select id from Dest1)
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
