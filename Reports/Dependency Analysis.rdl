  <DataSets>
    <DataSet Name="DataSource1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RunKey">
            <Value>=Parameters!RunKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     dbo.SourceTables.ObjectKey AS TableObjectKey, dbo.SourceTables.ObjectName AS TableName, 
                      srel.ParentObjectKey AS ConnectionKey, sconn.ConnectionName AS ConnectionName, 
                      sconn.ConnectionString AS ConnectionString, sconn.Server AS ServerName, sconn.[Database] AS DatabaseName, 
                      dbo.SourceTables.SrcComponentKey AS ComponentKey, dbo.SourceTables.DataFlowID, dbo.SourceTables.SourceObjectsName AS ComponentName
FROM dbo.ObjectRelationships AS srel 
INNER JOIN dbo.SourceTables 
	ON dbo.SourceTables.ObjectKey = srel.ChildObjectKey
	AND dbo.SourceTables.RunKey = srel.RunKey
	AND srel.RunKey = @RunKey
INNER JOIN dbo.Connections AS sconn 
	ON sconn.ConnectionID = srel.ParentObjectKey
	AND sconn.RunKey = srel.RunKey

UNION ALL
SELECT     dbo.TargetTables.ObjectKey, dbo.TargetTables.ObjectName, srel.ParentObjectKey, 
                      tconn.ConnectionName, tconn.ConnectionString, tconn.Server, 
                      tconn.[Database], dbo.TargetTables.TgtComponentKey , dbo.TargetTables.DataFlowID, 
                      dbo.TargetTables.TargetComponentName
FROM dbo.Connections AS tconn 
INNER JOIN dbo.ObjectRelationships AS srel  
	ON tconn.ConnectionID = srel.ParentObjectKey
	AND tconn.RunKey = srel.RunKey
	AND srel.RunKey = @RunKey
INNER JOIN dbo.TargetTables 
	ON dbo.TargetTables.ObjectKey = srel.ChildObjectKey
	AND dbo.TargetTables.RunKey = srel.RunKey
order by ConnectionKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
